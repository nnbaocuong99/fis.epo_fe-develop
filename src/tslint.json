{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "kt",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "indent": [
      true,
      "spaces",
      4
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  }
}